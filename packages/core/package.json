{
  "name": "@kopai/core",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "generate-otlp-zod-schema": "ts-to-zod src/otlp-generated.ts src/otlp-zod.ts && pretty-quick"
  },
  "devDependencies": {
    "@kopai/tsconfig": "workspace:*",
    "pretty-quick": "^4.2.2",
    "ts-to-zod": "^5.1.0",
    "tsdown": "^0.19.0"
  }
}
